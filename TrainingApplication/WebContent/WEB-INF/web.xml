<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>TrainingApplication</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>JdbcConnection</display-name>
    <servlet-name>JdbcConnection</servlet-name>
    <servlet-class>p1.JdbcConnection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JdbcConnection</servlet-name>
    <url-pattern>/JdbcConnection</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ShowTrainingServlet</display-name>
    <servlet-name>ShowTrainingServlet</servlet-name>
    <servlet-class>com.trainingapplication.ui.ShowTrainingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowTrainingServlet</servlet-name>
    <url-pattern>/ShowTrainingServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ResponseServlet</display-name>
    <servlet-name>ResponseServlet</servlet-name>
    <servlet-class>com.trainingapplication.ui.ResponseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResponseServlet</servlet-name>
    <url-pattern>/ResponseServlet</url-pattern>
  </servlet-mapping>
</web-app>